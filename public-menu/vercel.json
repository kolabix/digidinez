{
    "buildCommand": "npm run build:prod",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/menu/:restaurantId",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, s-maxage=86400, stale-while-revalidate=59"
                }
            ]
        }
    ]
}